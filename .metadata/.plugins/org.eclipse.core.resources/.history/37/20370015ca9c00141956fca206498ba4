package edu.rits.ma.algorithm.nashequil;

import java.util.ArrayList;
import java.util.List;

import edu.rits.ma.common.abstr.ITask;
import edu.rits.ma.common.theory.IPreference;
import edu.rits.ma.common.theory.IPreferenceSet;
import edu.rits.ma.common.theory.IUtilitiesMap;

public class FindBestPreferencesTaskImpl implements ITask {

	private int mPrimaryAgentId;
	private IPreference mSubPreference;
	private IPreferenceSet mPreferenceSet;
	private IUtilitiesMap mUtilitiesMap;
	private List<IPreference> mCandidatePreferences = new ArrayList<IPreference>();
	
	public FindBestPreferencesTaskImpl(int primaryAgentId, IPreference subPreference, IPreferenceSet preferenceSet) {
		mPrimaryAgentId = primaryAgentId;
		mSubPreference = subPreference;
		mPreferenceSet = preferenceSet;
	}
	
	@Override
	public long getTimeOutMs() {
		return 1000;
	}

	@Override
	public void execute() {
		List<IPreference> subPreferenceContainers = new ArrayList<IPreference>();
		mPreferenceSet.getPreferencesContains(mSubPreference, subPreferenceContainers);
		for(IPreference preference : subPreferenceContainers) {
			
		}
	}

	@Override
	public int getStatus() {
		return 0;
	}

	@Override
	public Object[] getResults() {
		return mCandidatePreferences.toArray();
	}

	@Override
	public void createSubTasks(List<ITask> subTasks) {
		
	}

}
