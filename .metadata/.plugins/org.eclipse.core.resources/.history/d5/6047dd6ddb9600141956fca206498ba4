package agents;

public class AgentStartNotifierImpl implements IAgentStartNotifier{

	private boolean mStarted = false;
	
	@Override
	public synchronized void waitForAgentStart() {
		while (!mStarted) {
			try {
				wait();
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}

	@Override
	public synchronized void nofityAllAgentControllers() {
		
	}

}
