package edu.rits.ma.common;

public interface ITask {
	public static final int TASK_STATUS_CREATE = -1;
	public static final int TASK_STATUS_INITIAED = 0;
	public static final int TASK_STATUS_FINISHED = 1;
	public static final int TASK_STATUS_SUCCEEDED = 2;
	public static final int TASK_STATUS_FAILED = 3;
	
	void execute();
	int getStatus();
	void createSubTasks(List<>);
}
