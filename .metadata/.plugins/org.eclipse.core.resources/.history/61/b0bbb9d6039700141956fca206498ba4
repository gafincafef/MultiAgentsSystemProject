package edu.rits.ma.behaviour;

import jade.core.Agent;
import jade.core.behaviours.Behaviour;
import edu.rits.ma.theory.IPreferenceSet;

public class PrimaryBehaviour extends Behaviour {

	/**
	 * 
	 */
	private static final long serialVersionUID = 5922438859910003521L;
	
	private IPreferenceSet mPreferenceSet = null;
	
	public PrimaryBehaviour(Agent agent, IPreferenceSet preferenceSet) {
		myAgent = agent;
		mPreferenceSet = preferenceSet;
	}
	
	@Override
	public void action() {
		
	}

	@Override
	public boolean done() {
		return false;
	}

}
