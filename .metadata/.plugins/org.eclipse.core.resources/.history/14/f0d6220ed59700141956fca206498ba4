package edu.rits.ma.jade.agent;

public class AgentOrganizer {
	public void setupAgent(IAgent agent, ICondVarReleaser condVarRelease) {
		agent.onSetupStart();
		
		try {
			onSetupArgumentsParsed(getArguments());
		}
		catch(Exception e) {
			e.printStackTrace();
			doDelete();
		}
		
		onSetupEnd();
	}
}
