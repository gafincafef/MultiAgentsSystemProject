	public static final int TASK_STATUS_CREATE = -1;
	public static final int TASK_STATUS_INITIAED = -1;
	public static final int TASK_STATUS_FINISHED = -1;
	public static final int TASK_STATUS_SUCCEEDED = -1;
	public static final int TASK_STATUS_SUCCEEDED = -1;
	
	void execute();
	int getStatus();
}
