package edu.rits.ma.jade.communication;

import jade.content.ContentElement;

import java.util.ArrayDeque;
import java.util.Deque;

public class ContentElementIncomingBuffer {
	private Deque<ContentElement> mReceivedElements = new ArrayDeque<ContentElement>();
	
	public boolean hasReceivedData() {
		return !mReceivedMessages.isEmpty() || !mReceivedObjectMessages.isEmpty();
	}
}
